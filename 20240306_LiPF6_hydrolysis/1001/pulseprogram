# 14 "/opt/topspin3.6.2/exp/stan/nmr/lists/pp/zg30" 2


# 17 "/opt/topspin3.6.2/exp/stan/nmr/lists/pp/zg30"
"p0=p1/3"


"acqt0=-tan((p0/p1)*(PI/4))*p1*2/PI"
# 1 "mc_line 23 file /opt/topspin3.6.2/exp/stan/nmr/lists/pp/zg30 exp. def. part of mc cmd. before ze"
; dimension 1D; AQ_mode 
define delay MCWRK
define delay MCREST
"MCREST = 30m - 30m"
"MCWRK = 0.333333*30m"

    dccorr
# 23 "/opt/topspin3.6.2/exp/stan/nmr/lists/pp/zg30"
# 23 "/opt/topspin3.6.2/exp/stan/nmr/lists/pp/zg30"
1 ze
# 1 "mc_line 23 file /opt/topspin3.6.2/exp/stan/nmr/lists/pp/zg30 exp. def. of mc cmd. after ze"
# 24 "/opt/topspin3.6.2/exp/stan/nmr/lists/pp/zg30"
# 1 "mc_line 24 file /opt/topspin3.6.2/exp/stan/nmr/lists/pp/zg30 exp. start label for mc cmd."
2 MCWRK  * 2
LBLF0, MCWRK
  MCREST
# 25 "/opt/topspin3.6.2/exp/stan/nmr/lists/pp/zg30"
  d1
  p0 ph1
  go=2 ph31
# 1 "mc_line 28 file /opt/topspin3.6.2/exp/stan/nmr/lists/pp/zg30 exp. mc cmd. in line"
  MCWRK wr #0
  MCWRK zd
  lo to LBLF0 times td0

# 29 "/opt/topspin3.6.2/exp/stan/nmr/lists/pp/zg30"
exit


ph1=0 2 2 0 1 3 3 1
ph31=0 2 2 0 1 3 3 1
# 36 "/opt/topspin3.6.2/exp/stan/nmr/lists/pp/zg30"
;pl1 : f1 channel - power level for pulse (default)
;p1 : f1 channel -  90 degree high power pulse
;d1 : relaxation delay; 1-5 * T1
;ns: 1 * n, total number of scans: NS * TD0


